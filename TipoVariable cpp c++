	// 3. Entero Largo ( long int ) 
	long int numeroLargo = 5533783605L; 
	cout << "Entero Largo : " << numeroLargo << endl;
	unsigned int numeroSinSigno = 255

	unsigned short int numeroCortoSinSigno = -32768 a 32768

